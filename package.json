{
  "name": "jsonapi-checker-ui",
  "version": "0.0.1",
  "description": "UI client for the jsonapi checker server",
  "author": "YvesPasteur",
  "license": "MIT",
  "main": "src/index.js",
  "dependencies": {
    "json-api-store": "^0.7.0",
    "node-static": "*",
    "react": "0.13.1",
    "react-ace": "^3.2.0",
    "throttle-debounce": "^0.1.1"
  },
  "devDependencies": {
    "browserify": "9.0.3",
    "envify": "3.4.0",
    "reactify": "1.1.0",
    "uglify-js": "2.4.17",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "watch": "node_modules/.bin/watchify src/index.js -o ./public/browser-bundle.js -v -d .",
    "watch-vm": "node_modules/.bin/watchify src/index.js --poll=1000 -o ./public/browser-bundle.js -v -d .",
    "start": "node server.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
